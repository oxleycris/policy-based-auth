@model PBA.Models.ViewModels.Admin.UsersViewModel
@{
    ViewData["Title"] = "Users";
}

<h2>Users</h2>

<ul>
    @foreach (var user in Model.Users)
    {
        <li>@string.Format("{0} {1}", user.GivenName, user.FamilyName) <a href="@Url.Action("EditUser", new { id = user.Id })">[Edit]</a></li>
    }
</ul>
